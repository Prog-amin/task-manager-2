# Server
NODE_ENV=development
PORT=4000

# Database (Neon)
DATABASE_URL=

# Auth
JWT_ACCESS_SECRET=replace_with_at_least_16_chars
JWT_ACCESS_EXPIRES_IN=15m
AUTH_COOKIE_NAME=access_token

# CORS
# Comma-separated list
CORS_ORIGINS=http://localhost:5173

# Cookies
# Set true on Railway production
COOKIE_SECURE=false
